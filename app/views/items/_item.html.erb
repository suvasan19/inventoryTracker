<div id="<%= dom_id item %>">
  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= item.date %>
  </p>

  <p>
    <strong>Customer:</strong>
    <%= item.customer %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= item.location_id %>
  </p>

  <p>
    <strong>Cost:</strong>
    <%= item.cost %>
  </p>

</div>
